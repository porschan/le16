<!--pages/index/index.wxml-->
<view class="page">
  <view style="height:200rpx;width:100%;background: url(/common/images/card.png) no-repeat;background-size: 100% 100%;" mode="aspectFill">
    <div class="headNav">
      <image class='touimage' src='/common/images/avatar.png' style="display: block" catchtap='operateList'></image>
      <text style="margin-left:30rpx;font-size:30rpx;color:#ffffff;display: flex; align-items: center; justify-content: center;">Tom</text>
    </div>
  </view>
  <view class="page__bd page__bd_spacing" style="padding-top:20rpx;">
      <view class="weui-cells__title">用户</view>
      <view class="weui-flex">
          <view class="weui-flex__item">
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell weui-cell_access" catchtap='toLogin'>
                    <view class="weui-cell__bd">
                      <image src="/common/images/check.png" style="width:40rpx;height:40rpx;margin-right:10rpx;vertical-align:middle;"></image>
                      <view style="display: inline-block; vertical-align: middle">登录管理</view>
                        <!-- <view class="weui-badge" style="margin-left: 5px;">8</view> -->
                    </view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </view>

                  <view class="weui-cell weui-cell_access" catchtap='toRentPage'>
                    <view class="weui-cell__bd">
                      <image src="/common/images/check.png" style="width:40rpx;height:40rpx;margin-right:10rpx;vertical-align:middle;"></image>
                      <view style="display: inline-block; vertical-align: middle">月租详情</view>
                        <!-- <view class="weui-badge" style="margin-left: 5px;">8</view> -->
                    </view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </view>

                  <view class="weui-cell weui-cell_access" catchtap='toMessagePage'>
                    <view class="weui-cell__bd">
                      <image src="/common/images/check.png" style="width:40rpx;height:40rpx;margin-right:10rpx;vertical-align:middle;"></image>
                      <view style="display: inline-block; vertical-align: middle">资讯</view>
                        <!-- <view class="weui-badge" style="margin-left: 5px;">8</view> -->
                    </view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </view>

                <view class="weui-cell weui-cell_access" catchtap='toWiFiListPage'>
                    <view class="weui-cell__bd">
                      <image src="/common/images/wifi.png" style="width:40rpx;height:40rpx;margin-right:10rpx;vertical-align:middle;"></image>
                      <view style="display: inline-block; vertical-align: middle">WiFi</view>
                        <!-- <view class="weui-badge" style="margin-left: 5px;">8</view> -->
                    </view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </view>


                <view class="weui-cell weui-cell_access" catchtap='toOtherListPage'>
                    <view class="weui-cell__bd">
                      <image src="/common/images/other.png" style="width:40rpx;height:40rpx;margin-right:10rpx;vertical-align:middle;"></image>
                      <view style="display: inline-block; vertical-align: middle">SSR</view>
                        <!-- <view class="weui-badge" style="margin-left: 5px;">8</view> -->
                    </view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </view>

            </view>
          </view>
      </view>

  </view>
</view>